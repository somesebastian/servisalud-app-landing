{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "redirects": [
    {
      "source": "/share/:id",
      "destination": "https://play.google.com/store/apps/details?id=com.servicard.servisalud",
      "permanent": false,
      "statusCode": 302
    },
    {
      "source": "/share/:id/:slug*",
      "destination": "https://play.google.com/store/apps/details?id=com.servicard.servisalud",
      "permanent": false,
      "statusCode": 302
    }
  ],
  "rewrites": [],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}