{
  "builds": [
    {
      "src": "dist/server/entry.mjs",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/server/entry.mjs"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}